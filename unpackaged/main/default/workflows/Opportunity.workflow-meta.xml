    <rules>
        <fullName>Loss Reason w%2Fo Closed Lost</fullName>
        <actions>
            <name>Remove_Loss_Reason</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Remove_Loss_Reason_Detail</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Remove_Sub_Loss_Reason</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <booleanFilter>1 AND (2 OR 3 OR 4)</booleanFilter>
        <criteriaItems>
            <field>Opportunity.StageName</field>
            <operation>notEqual</operation>
            <value>Closed Lost</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.Loss_Reason__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.Sub_Loss_Reason__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.Loss_Reason_Detail__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <description>Intention is to find Opps that are moved away from Closed Lost, but still have Loss Reasons and set those to null</description>
        <triggerType>onAllChanges</triggerType>
    </rules>
</Workflow>
