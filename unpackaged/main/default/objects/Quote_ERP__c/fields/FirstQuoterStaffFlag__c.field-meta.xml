<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FirstQuoterStaffFlag__c</fullName>
    <description>Checks if the quote creator is staff</description>
    <externalId>false</externalId>
    <formula>(CONTAINS( LOWER(FirstQuoterEmail__c), '@xometry')) ||
(CONTAINS( LOWER(FirstQuoterEmail__c), '@thomasnet.com'))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>First Quoter Staff Flag</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
