<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>IsOpenOnWeekend__c</fullName>
    <description>If Case Opened on Sat or Sun.</description>
    <externalId>false</externalId>
    <formula>IF(WEEKDAY(DATEVALUE(CreatedDate))=1,true, IF(WEEKDAY(DATEVALUE(CreatedDate))=7,true, false))</formula>
    <inlineHelpText>If Case Opened on Sat or Sun.</inlineHelpText>
    <label>Is Opened On Weekend</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
