<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CMSLAResponseTimeReq_hrs__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
IsPartnerSupplier__c = TRUE,
CASE(Priority,
"Urgent",2,
"High",2,
"Low",3,
"Medium",3,
"Normal",3,3),
CASE(Priority,
"Urgent",2,
"High",2,
"Low",6,
"Medium",6,
"Normal",6,6))</formula>
