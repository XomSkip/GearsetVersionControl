<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Age_hrs__c</fullName>
    <description>Created During PSE Service Migration Project</description>
    <externalId>false</externalId>
    <formula>IF( ISBLANK(ClosedDate) ,
      (NOW() -  CreatedDate)*24,
      (ClosedDate - CreatedDate)*24
 )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>The number of hours between when the case was created and the current day/time or the case closed date if the case is closed.</inlineHelpText>
    <label>Age (hrs)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
