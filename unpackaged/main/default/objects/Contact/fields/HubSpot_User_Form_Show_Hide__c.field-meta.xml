<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>HubSpot_User_Form_Show_Hide__c</fullName>
    <description>Used to check the fields coming from the HubSpot User Profile Form for null values. If all fields are null the Action will not show on the Lightning Page.</description>
    <externalId>false</externalId>
    <formula>AND(
ISBLANK(HS_Applications__c),
ISBLANK(HS_Manufacturing_Processes_Utilized__c),
ISBLANK(HS_Manufacturing_Process_Interest__c),
ISBLANK(HS_Preferred_Manufacturing_Providers__c),
ISBLANK(HS_Preferred_Online_Provider__c),
ISBLANK(TEXT(HS_Purchase_Authority__c)),
ISBLANK(HS_Spending_Limit__c))</formula>
    <label>HubSpot User Form Show/Hide</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
