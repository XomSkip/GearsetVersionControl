<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>valideQuantityExpected</fullName>
    <active>true</active>
    <errorConditionFormula>Quantity_Expected__c &gt;  Ordered_Part_Id__r.QuantityOrdered__c</errorConditionFormula>
    <errorDisplayField>Quantity_Expected__c</errorDisplayField>
    <errorMessage>The expected quantity cannot exceed the ordered quantity of the ordered part</errorMessage>
</ValidationRule>
