<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Pacing_Stat__c</fullName>
    <externalId>false</externalId>
    <formula>if(Last_Year_Account_Spend__c = 0
,null
,if(isblank(Last_Year_Account_Spend__c)
,null
,This_Year_Account_Spend__c / ((today()-date(year(today())-1,12,31))/365* Last_Year_Account_Spend__c )))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>This is the net neutral account spend pacing percentage for This Year Spend against Last Year Spend.</inlineHelpText>
    <label>Pacing Stat</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Percent</type>
</CustomField>
