<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Retention_Countdown__c</fullName>
    <description>Calculates time to retention for BDRs</description>
    <externalId>false</externalId>
    <formula>IF(ISPICKVAL(Account_Owner_Type__c,"BDR"),(Date_of_First_Order__c+90)-TODAY(),null)</formula>
    <label>Account Retention Countdown</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
